<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:layout_marginTop="@dimen/_15sdp"
    tools:context=".categoryView">

  <com.makeramen.roundedimageview.RoundedImageView
      android:id="@+id/imageProfile"
      android:layout_width="@dimen/_25sdp"
      android:layout_height="@dimen/_25sdp"
      android:layout_marginStart="@dimen/_16sdp"
      android:layout_marginLeft="@dimen/_16sdp"
      android:layout_marginTop="@dimen/_15sdp"
      android:src="@drawable/img_52"
      app:riv_border_color="@color/black"
      android:scaleType="centerCrop"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:riv_corner_radius="@dimen/_8sdp"
      tools:ignore="MissingConstraints" />

  <com.makeramen.roundedimageview.RoundedImageView
      android:id="@+id/imageSignOut"
      android:layout_width="@dimen/_25sdp"
      android:layout_height="@dimen/_25sdp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="72dp"
      android:scaleType="centerCrop"
      android:src="@drawable/img_54"
      android:tint="@color/white"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      tools:ignore="MissingConstraints" />

  <com.makeramen.roundedimageview.RoundedImageView
      android:id="@+id/cart"
      android:layout_width="@dimen/_25sdp"
      android:layout_height="@dimen/_25sdp"
      android:layout_marginTop="@dimen/_12sdp"
      android:layout_marginEnd="@dimen/_16sdp"
      android:src="@drawable/img_55"
      android:scaleType="centerCrop"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      android:tint="@color/white"
      tools:ignore="MissingConstraints" />
  <ImageView
      android:id="@+id/firstImage"
      android:layout_width="111dp"
      android:layout_height="50dp"
      android:src="@drawable/img_53"
      app:layout_constraintEnd_toStartOf="@+id/imageSignOut"
      app:layout_constraintHorizontal_bias="0.58"
      app:layout_constraintStart_toEndOf="@+id/imageProfile"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.263"
      tools:ignore="MissingConstraints" />

  <ScrollView
      android:id="@+id/scroll"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_marginTop="@dimen/_16sdp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toBottomOf="@id/imageProfile"
      app:layout_constraintVertical_bias="0.0"
      tools:ignore="MissingConstraints"
      tools:layout_editor_absoluteX="0dp">






      <LinearLayout
          android:id="@+id/labeled"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:orientation="vertical"
          android:padding="16dp"
          app:layout_constraintTop_toBottomOf="@id/thirdImage"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintEnd_toEndOf="parent">



        <ImageView
            android:id="@+id/firstImage1"
            android:layout_width="match_parent"
            android:layout_height="522dp"
            android:src="@drawable/img_56"
            app:layout_constraintTop_toTopOf="parent"
             />


        <ImageView
            android:id="@+id/secondImage"
            android:layout_width="match_parent"
            android:layout_height="522dp"
            android:src="@drawable/img_58" />

        <ImageView
            android:id="@+id/thirdImage"
            android:layout_width="match_parent"
            android:layout_height="522dp"
            android:src="@drawable/img_60" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="270dp"
            android:background="@color/white"
            android:orientation="vertical">


          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="46dp"
              android:orientation="horizontal">

            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:src="@drawable/img_14" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:src="@drawable/img_15" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:src="@drawable/img_16" />


          </LinearLayout>


          <ImageView
              android:layout_width="match_parent"
              android:layout_height="101dp"
              android:layout_marginTop="@dimen/_10sdp"
              android:src="@drawable/img_17" />

          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginTop="@dimen/_20sdp"
              android:orientation="horizontal">

            <TextView
                android:id="@+id/text_view_about"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:text="About" />

            <TextView
                android:id="@+id/text_view_contact"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:text="Contact" />

            <TextView
                android:id="@+id/text_view_blog"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="blog"
                android:padding="8dp" />


          </LinearLayout>

          <TextView
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginTop="@dimen/_20sdp"
              android:gravity="center"
              android:text="CopyrightÂ© OpenUI All Rights Reserved." />


        </LinearLayout>



      </LinearLayout>






  </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>